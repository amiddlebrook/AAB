{
  "name": "aab-worker",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create aab-db",
    "db:migrate": "wrangler d1 execute aab-db --file=./worker/schema.sql",
    "db:migrate:local": "wrangler d1 execute aab-db --local --file=./worker/schema.sql"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.99.0"
  }
}